<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>


<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.saprbarbyme.Controller.HelloController"
      alignment="CENTER"
      spacing="10">

  <padding>
    <Insets top="20" right="20" bottom="20" left="20"/>
  </padding>
  <Button text="Препроцессор" onAction="#handlePreProcessor"/>
  <Button text="Процессор" onAction="#handleProcessor"/>
  <Button text="Постпроцессор" onAction="#handlePostProcessor"/>
</VBox>
